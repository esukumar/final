<div class="container">
    <h3>Sign In</h3>

    <%= form_tag sessions_url do %>

        <p>
          <%= label_tag :email %>
          <%= text_field_tag :email %>
        </p>

        <p>
          <%= label_tag :password %>
          <%= text_field_tag :password %>
        </p>

        <%= submit_tag "Let Me In!" %>
    <% end %>
</div>